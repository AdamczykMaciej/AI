    public LanguageSample(){};

    public LanguageSample(double a, double b, double c, double d, double e, double f, double g, double h, double i, double j, double k, double l, double m, double n, double o, double p,
    double q, double r, double s, double t, double u, double v, double w, double x, double y, double z, String className){
        this.setA(a);
        this.setB(b);
        this.setC(c);
        this.setD(d);
        this.setE(e);
        this.setF(f);
        this.setG(g);
        this.setH(h);
        this.setI(i);
        this.setJ(j);
        this.setK(k);
        this.setL(l);
        this.setM(m);
        this.setN(n);
        this.setO(o);
        this.setP(p);
        this.setQ(q);
        this.setR(r);
        this.setS(s);
        this.setT(t);
        this.setU(u);
        this.setV(v);
        this.setW(w);
        this.setX(x);
        this.setY(y);
        this.setZ(z);
        this.className=className;
    }

    public double getA() {
        return a;
    }

    public void setA(double a) {
        this.a = a;
    }

    public double getB() {
        return b;
    }

